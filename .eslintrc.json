{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "no-restricted-syntax": "off",
    "global-require": "off",
    "class-methods-use-this": "off",
    "max-classes-per-file": "off",
    "no-plusplus": "off",
    "radix": "off"
  }
}
